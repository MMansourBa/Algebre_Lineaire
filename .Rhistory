# 1. Formation de la matrice 5x5
matrice_5x5 <- matrix(c(2, 4, 3, 1, 3,
7, 2, 4, 1, 3,
7, 4, 5, 4, 4,
5, 5, 4, 4, 4,
1, 5, 3, 4, 2),
nrow = 5, byrow = TRUE)
# 2. Calculer le déterminant de la matrice
determinant_matrice <- det(matrice_5x5)
# 3. Calculer la matrice transposée
matrice_transposee <- t(matrice_5x5)
# 4. Calculer la matrice inverse
matrice_inverse <- solve(matrice_5x5)
# 5. Extraire l’élément à la 4e ligne et 3e colonne
element_4_3 <- matrice_5x5[4, 3]
# 6. Afficher la 3e ligne
ligne_3 <- matrice_5x5[3, ]
# 7. Afficher la 4e colonne
colonne_4 <- matrice_5x5[, 4]
# 8. Former les matrices M (5x4) et N (4x8), puis calculer le produit M × N
M <- matrice_5x5[, 1:4]  # Matrice M (5x4)
N <- matrix(1:32, nrow = 4)  # Matrice N (4x8) avec des valeurs quelconques
produit_MN <- M %*% N
# Afficher les résultats
print(list(
Matrice_5x5 = matrice_5x5,
Determinant = determinant_matrice,
Matrice_transposee = matrice_transposee,
Matrice_inverse = matrice_inverse,
Element_4_3 = element_4_3,
Ligne_3 = ligne_3,
Colonne_4 = colonne_4,
Matrice_M = M,
Matrice_N = N,
Produit_M_N = produit_MN
))
